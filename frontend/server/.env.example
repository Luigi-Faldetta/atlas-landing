# Server port
PORT=5000

# OpenAI API key (optional)
OPENAI_API_KEY=

# Enable/disable caching (1 = enabled, 0 = disabled)
ENABLE_CACHE=1

# Cache time-to-live in milliseconds (1 hour default)
CACHE_TTL=3600000

# Puppeteer options
PUPPETEER_HEADLESS=true

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX_REQUESTS=20   # 20 requests per 15 minutes

# Proxies (comma-separated, optional)
# Format: http://username:password@host:port
PROXY_LIST= 